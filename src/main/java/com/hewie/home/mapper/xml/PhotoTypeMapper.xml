<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hewie.home.mapper.PhotoTypeMapper">

    <select id="selectPhotoByTypeId" resultType="com.hewie.home.entity.Photo">
        select *
        from home.t_photo where t_photo.type_id = #{id}
    </select>
</mapper>
